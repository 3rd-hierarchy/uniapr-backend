{
  "name": "uniapr",
  "version": "1.0.1",
  "repository": "git@github.com:senioris/uniapr-backend.git",
  "author": "Senioris",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --project ./tsconfig.json src/index.ts",
    "build": "./node_modules/.bin/tsc.cmd -b",
    "start": "node ./dist/index.js",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "heroku-postbuild": "tsc -b",
    "startl": "SET NODE_ENV=production && node ./dist/index.js",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "async-mutex": "^0.3.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "helmet": "^4.4.1",
    "log4js": "^6.3.0",
    "mongoose": "^5.11.19",
    "node-cron": "^3.0.0",
    "node-schedule": "^2.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/graphql": "^14.5.0",
    "@types/helmet": "^4.0.0",
    "@types/log4js": "^2.3.5",
    "@types/node": "^14.14.31",
    "@types/node-cron": "^2.0.3",
    "@types/node-schedule": "^1.3.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  }
}
